<!--NEW FAB Adblock Check-->
<script type="text/javascript">
	var arrDevices = [ 'desktop', 'tablet', 'mobile'];
	function adBlockNotDetected() {
		device =  MentalFloss.getCurrentDevice();
		if(jQuery.inArray( device, arrDevices ) != -1){
			MentalFloss.pushUAEvent('FAB New Adblock', 'Unblocked', 'Unblocked-'+device+'-Ads', 0);
		}
	}
	function adBlockDetected() {
		device =  MentalFloss.getCurrentDevice();
		if(jQuery.inArray( device, arrDevices ) != -1){
			MentalFloss.pushUAEvent('FAB New Adblock', 'Blocked', 'Blocked-'+device+'-Ads', 0);
		}
	}

	if(typeof fAB === 'undefined') {
		adBlockDetected();
	} else {
		fAB.onDetected(adBlockDetected);
		fAB.onNotDetected(adBlockNotDetected);
	}
</script>
